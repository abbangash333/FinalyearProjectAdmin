<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.finalyearprojectadmin">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".addDriver.DeleteDriverDetail"
            android:parentActivityName=".addDriver.AddDriver"></activity>
        <activity
            android:name=".addDriver.AddDriver"
            android:parentActivityName=".HomeDashBoard" />
        <activity android:name=".SignIn">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ForgotPassword"
            android:parentActivityName=".SignIn" />
        <activity
            android:name=".HomeDashBoard"
            android:parentActivityName=".SignIn" />
    </application>

</manifest>